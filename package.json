{
  "name": "@lfai/egeria-ui-components",
  "version": "4.2.17",
  "description": "Encapsulated reactjs components with business logic inside.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/odpi/egeria-ui-components.git"
  },
  "files": [
    "dist",
    "src/**/**.scss"
  ],
  "main": "./dist/esm/main.js",
  "module": "./dist/esm/main.js",
  "types": "./dist/types/main.d.ts",
  "scripts": {
    "start": "react-scripts start",
    "build:demo": "react-scripts build",
    "build": "tsc -p tsconfig.lib.json",
    "postbuild": "npm run copy:assets",
    "copy:assets": "cpx 'src/**/**.scss' 'dist/esm/'",
    "test": "react-scripts test --watchAll=false --passWithNoTests --transformIgnorePatterns 'node_modules/(?!@lfai)/'",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx,json}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/serialize": "^1.1.1",
    "@lfai/egeria-js-commons": "^4.2.17",
    "@lfai/egeria-ui-core": "^4.2.8",
    "@lfai/happi-graph": "^4.1.2",
    "@mantine/core": "^5.6.3",
    "@mantine/hooks": "^5.6.3",
    "@mantine/notifications": "^5.6.4",
    "ag-grid-community": "^28.1.1",
    "ag-grid-react": "^28.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tabler-icons-react": "1.56.0",
    "web-vitals": "3.1.1",
    "web-worker": "1.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.4",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "cpx": "^1.5.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "typescript": "^4.8.4"
  }
}
